{#def
    title: str = "",

    current_stage: str,
    stages: dict,

    form
#}

<setup.Setup current_stage={{ current_stage }} stages={{ stages }}>
    <form action="" method="post" novalidate>
        {{ form.hidden_tag() }}
        
        <div class="
            py-8 leading-6 space-y-8 sm:leading-7
            text-base text-on-surface sm:text-lg
        ">
        
        </div>

        <div class="flex justify-between mt-12">
            <ui.buttons.Button href={{ url_for("setup.libraries") }}>Back</ui.buttons.Button>
            <div class="flex gap-2 items-center">
                {% if form.errors %}<p class="text-error">Correct errors before you try again.</p>{% endif %}
                <form.submit.Submit primary error={{ form.errors }} form={{ form }} />
            </div>
        </div>
    </form>
</setup.Setup>