{#def
    skippable: bool = False,
    form
#}

<setup.Setup>
    <form action="" method="post" novalidate>
        {{ form.hidden_tag() }}

        <div class="flex flex-col gap-8 px-10">
            <h2 class="my-5 font-thin text-6xl text-center text-primary">Libraries</h2>
            
            {# <div class="flex flex-col gap-8">
                
            </div> #}

            <ui.containers.Card variant additional_class="p-2">
                <ui.containers.Card variant>
                    <js_tree.JSTree />
                </ui.containers.Card>
                <ui.buttons.Button additional_class="w-full">
                    <p class="text-4xl">+</p>
                </ui.buttons.Button>
            </ui.containers.Card>

            <div class="flex justify-between my-5">
                <ui.buttons.Button href={{ url_for("setup.authentication") }}>Back</ui.buttons.Button>
                <div class="flex gap-2 items-center">
                    {% if form.errors %}<p class="text-sm text-error">Correct errors before you try again.</p>{% endif %}
                    {% if skippable %}<ui.buttons.Button href={{ url_for("setup.libraries") }}>Skip</ui.buttons.Button>{% endif %}
                    <ui.forms.Submit primary error={{ form.errors }} form={{ form }} />
                </div>
            </div>
        </div>
    </form>
</setup.Setup>

<script>

</script>